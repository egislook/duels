<div class="page">
    <table cellspacing="0" class="spaced textLight">
        <tr>
            <td class="b-r" height="20px"></td>
        </tr>
        <tr>
            <td class="b-r">
                <div class="dark right textSmall">{{= it.g.info.date}}</div>
            </td>
        </tr>
        <tr>
            <td class="b-r">
                <div class="dark right textSmall up_case">{{= it.g.info.mode}}</div>
            </td>
        </tr>
        <tr>
            <td class="b-r" height="3px"></td>
        </tr>
        <tr>
            <td class="b-r-r">
                {{? it.g.info.mode == 'sh' || it.g.info.mode == 'ar'}}
                <img class="right hero" src="http://media.steampowered.com/apps/dota2/images/heroes/{{=it.g.t1[0].hero.url}}_sb.png"/>
                {{?}}
                <div class="dark right b-a">{{=it.u[it.g.t1[0].id].name}}</div>
                <img class="right b-a avatar" src="{{=it.u[it.g.t1[0].id].avatar}}"/>
            </td>
            <td>
                {{? it.g.info.mode == 'sh' || it.g.info.mode == 'ar'}}
                <img class="left hero" src="http://media.steampowered.com/apps/dota2/images/heroes/{{=it.g.t2[0].hero.url}}_sb.png"/>
                {{?}}
                <div class="dark left b-a">{{=it.u[it.g.t2[0].id].name}}</div>
                <img class="left b-a avatar" src="{{=it.u[it.g.t2[0].id].avatar}}"/>
            </td>
        </tr>
        {{? it.user.loged && it.user.status === 'good'}}
            <form action="../decide" method="post">
                <tr>
                    <input name="id" type="hidden" value="{{=it.gameid}}">
                    <td class="b-r" height="10px"></td>
                </tr>
                <tr>
                    <td colspan="2" align="center">
                        <select class="inputs b-a" name="win">
                            {{? it.g.info.win=='t1'}}
                                <option value="t1" selected> <<<<<< radiant </option>
                            {{??}}
                                <option value="t1"> <<<<<< radiant </option>
                            {{?}}
                            {{? it.g.info.win=='t2'}}
                                <option value="t2" selected> dire >>>>>> </option>
                            {{??}}
                                <option value="t2"> dire >>>>>> </option>
                            {{?}}
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="b-r" height="10px"></td>
                </tr>
                    <tr>
                        <td class="b-r">
                            <div class="dark right textSmall">duration</div>
                        </td>
                        <td>
                            {{ if(it.g.info.duration){var val=it.g.info.duration;}else{var val='0';}}}
                            <input name="duration" class="inputs left" size="3" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="10px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="dark textSmall" style="display:inline-block">level</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].lvl){var val=it.g.t1[0].lvl;}else{var val='1';}}}
                            <input name="lvl[]" class="inputs b-a nb-r right" size="1" value="{{=val}}">
                            
                        </td>
                        <td>
                            {{ if(it.g.t2[0].lvl){val=it.g.t2[0].lvl;}else{val='1';}}}
                            <input name="lvl[]" class="inputs left" size="1" value="{{=val}}">
                        </td>
                    </tr>
                    
                    <tr>
                        <td class="b-r" height="2px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="accept textSmall" style="display:inline-block">kills</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].k){val=it.g.t1[0].k;}else{val='0';}}}
                            <input name="k[]" class="inputs b-a nb-r right" size="1" value="{{=val}}">
                        </td>
                        <td>
                            {{ if(it.g.t2[0].k){val=it.g.t2[0].k;}else{val='0';}}}
                            <input name="k[]" class="inputs left" size="1" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="2px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="delete textSmall" style="display:inline-block">deaths</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].d){val=it.g.t1[0].d;}else{val='0';}}}
                            <input name="d[]" class="inputs b-a nb-r right" size="1" value="{{=val}}">
                        </td>
                        <td>
                            {{ if(it.g.t2[0].d){val=it.g.t2[0].d;}else{val='0';}}}
                            <input name="d[]" class="inputs left" size="1" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="8px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="dark textSmall" style="display:inline-block">last hits</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].last){val=it.g.t1[0].last;}else{val='0';}}}
                            <input name="last[]" class="inputs b-a nb-r right" size="1" value="{{=val}}">
                        </td>
                        <td>
                            {{ if(it.g.t2[0].last){val=it.g.t2[0].last;}else{val='0';}}}
                            <input name="last[]" class="inputs left" size="1" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="2px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="dark textSmall" style="display:inline-block">denies</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].denie){val=it.g.t1[0].denie;}else{val='0';}}}
                            <input name="denie[]" class="inputs b-a nb-r right" size="1" value="{{=val}}">
                        </td>
                        <td>
                            {{ if(it.g.t2[0].denie){val=it.g.t2[0].denie;}else{val='0';}}}
                            <input name="denie[]" class="inputs left" size="1" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="8px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="edit textSmall" style="display:inline-block">gold/min</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].gold){val=it.g.t1[0].gold;}else{val='0';}}}
                            <input name="gold[]" class="inputs b-a nb-r right" size="2" value="{{=val}}">
                        </td>
                        <td>
                            {{ if(it.g.t2[0].gold){val=it.g.t2[0].gold;}else{val='0';}}}
                            <input name="gold[]" class="inputs left" size="2" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="2px"></td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div class="dark textSmall" style="display:inline-block">exp/min</div>
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r">
                            {{ if(it.g.t1[0].exp){val=it.g.t1[0].exp;}else{val='0';}}}
                            <input name="exp[]" class="inputs b-a nb-r right" size="2" value="{{=val}}">
                        </td>
                        <td>
                            {{ if(it.g.t2[0].exp){val=it.g.t2[0].exp;}else{val='0';}}}
                            <input name="exp[]" class="inputs left" size="2" value="{{=val}}">
                        </td>
                    </tr>
                    <tr>
                        <td class="b-r" height="10px"></td>
                    </tr>
                    <tr>
                        <td class="b-r"></td>
                        <td>
                            <input type="submit" class="edit click left b-t dest textLight" value="save stats"/>
                        </td>
                    </tr>
                </form>
            {{??}}
            <tr>
                <td class="b-r" height="10px"></td>
            </tr>
            <tr>
                <td colspan="2" align="center">
                  <div class="b-a neutral">"?" Victory</div>
                </td>
            </tr>
            <tr>
                <td class="b-r" height="10px"></td>
            </tr>
